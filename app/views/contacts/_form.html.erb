<div class="field3">
  <%= f.label :protocol %><br />
  <%= f.select(:protocol, Contact::ENUM, {} ,{:disabled=> (defined? disabled and disabled)}) %>
</div>
<div class="field3">
  <%= f.label :value %><br />
  <%= f.text_field :value, {:disabled=>(defined? disabled and disabled)} %>
</div>
<div class="field3">
  <% if f.object.id.nil? %>
    &nbsp;&nbsp;<%= link_to_remove_fields t(:destroy),f %>
  <% else %>
    <%= f.check_box :_destroy, {:disabled=>(defined? disabled and disabled)} %>
    <%= f.label :_destroy, t(:destroy) %>
  <% end %>
</div>
