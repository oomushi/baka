<% case contact.protocol %>
 <% when 'email' %>
   <%= mail_to contact.value,image_tag("theme-default/email.svg", :alt => "email"), { :encode=>:javascript, :title=> contact.value,:class=>"button"} %>
 <% when 'website' %> 
   <%= link_to image_tag("theme-default/web.svg", :alt => "web"),contact.value, {class: 'web button', target: '_blank', title: contact.value} %>
<% end %>
